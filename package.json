{
  "name": "the-tricktionary",
  "version": "3.0.0",
  "private": true,
  "description": "A list of jumprope tricks with name, description and video",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "firebase deploy --only hosting,firestore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@sentry/browser": "^5.19.2",
    "@sentry/integrations": "^5.19.2",
    "awesome-phonenumber": "^2.35.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.6.5",
    "firebase": "^7.16.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-fuse": "^2.2.1",
    "vue-google-adsense": "^1.8.3",
    "vue-gtagjs": "^1.0.7",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "^3.3.4",
    "vue-script2": "^2.1.0",
    "vue-tel-input": "^4.4.0",
    "vue-youtube": "^1.4.0",
    "vuex": "^3.5.1",
    "vuex-easy-firestore": "^1.35.14",
    "vuex-persist": "^2.2.0",
    "vuex-shared-mutations": "^1.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-pwa": "^4.4.6",
    "@vue/cli-plugin-typescript": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "markdown": "^0.5.0",
    "postcss": "^7.0.32",
    "postcss-css-variables": "^0.17.0",
    "typescript": "~3.9.3",
    "vue-markdown": "^2.2.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript",
      "@vue/standard"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "globals": {
      "Stripe": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-css-variables": {
        "preserve": true
      }
    }
  },
  "browserslist": [
    "cover 99.5%"
  ],
  "homepage": "https://the-tricktionary.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "the-tricktionary/web"
  }
}

{
  "rules": {
    "tricks": {
      ".read": true,
      ".write": false
    },
    "news": {
      ".read": true,
      ".write": false
    },
    "submissions": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "contact": {
      ".read": true,
      ".write": true
    },
    "speed": {
      "scores": {
        "$uid": {
          ".write": "$uid == auth.uid"
        },
        ".read": true
      },
      "events": {
        "$uid": {
          ".write": "$uid == auth.uid",
          "$id": {
            "pools": {
              ".validate": "newData.isNumber()"
            },
            "heats": {
              ".validate": "newData.isNumber()"
            }
          }
        },
        ".read": true
      }
    }
  }
}